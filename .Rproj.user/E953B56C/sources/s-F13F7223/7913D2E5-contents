load("./trolls.Rdata")
require(network)

get.vertex.attribute(trolls)
list.vertex.attributes(trolls)


ll=sapply(list.vertex.attributes(trolls), function(a) length(get.vertex.attribute(trolls,a)))

names(ll[ll==1245])

X=sapply(names(ll[ll==1245]), function(a) (get.vertex.attribute(trolls,a)))
tmp = data.frame(X,stringsAsFactors = T)
tmp$vertex.names
Y = as.edgelist(x = trolls)
save(list = c("X", "Y"),file = "russian_trolls.RData")
save(list = c("X", "Y"),file = "russian_trollsV2.RData",version = 2)
